package module;

public class Data {

	public String firstname;
	public String lastname;
     int id;
     String mail;
     String password;
     String dob;
     
     String username;
     String cpassword;
	public Data(String firstname, String lastname, int id, String mail, String password, String dob,String username,String cpassword) {
		super();
		this.firstname = firstname;
		this.lastname = lastname;
		this.id = id;
		this.mail = mail;
		this.password = password;
		this.dob = dob;
		this.username=username;
		this.cpassword=password;
	}
	public Data() {
		super();
	}
     
     //Data members of class
     
     public void display() {
    	 
     }
      
	
}
