import java.io.*;
public class CopyFile1 {

   public static void main(String args[]) throws IOException {
      FileReader in = null;
      FileWriter out = null;

      try {
         in = new FileReader("D:\\Govind Singh\\niit_tech_logo.jpg");
         out = new FileWriter("output.jpg");
         
         int c;
         while ((c = in.read()) != -1) {
            out.write(c);
         }
      }finally {
    	  
         if (in != null) {
            in.close();
         }
         if (out != null) {
            out.close();
         }
      }
      
      System.out.println("file copied");
   }
}
