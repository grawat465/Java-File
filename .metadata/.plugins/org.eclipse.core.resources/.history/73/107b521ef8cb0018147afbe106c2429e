	public boolean validateFirstName(String a) {
		
		if(Pattern.matches("[A-Za-z]{3,7}",a))
		{
			return true;
		}
		else return false;
		
		//Data signup=new Data();
		public boolean validateLastName(String a) {
			
			if(Pattern.matches("[A-Za-z]{3,7}",a))
			{
				return true;
			}
			else return false;
		
		
	}
		
		
