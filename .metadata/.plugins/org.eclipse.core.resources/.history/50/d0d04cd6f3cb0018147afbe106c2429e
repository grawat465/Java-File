package service;
import module.Data;
import java.util.regex.Pattern;

public class Validation {
	Data signup=new Data();
	
	//Data signup=new Data();
	public boolean validate() {
		
		if(Pattern.matches("[A-Z a-z] {3-7}",signup.firstname))
		{
			return true;
		}
		else return false;
	
		
	
		
		
	}
		
		
	}


	
